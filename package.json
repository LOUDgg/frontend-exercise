{
  "name": "frontend-exercise",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "yarn db:generate",
    "test": "jest",
    "db:save": "dotenv -- yarn prisma migrate save --experimental",
    "db:migrate": "dotenv -- yarn prisma migrate up --experimental",
    "db:generate": "dotenv -- yarn prisma generate",
    "clean": "rimraf dist",
    "prebuild": "yarn clean && yarn db:generate",
    "build": "cross-env NODE_ENV=production tsc ",
    "predev": "yarn clean && yarn db:generate",
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --rs src/index.ts --no-notify",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@prisma/cli": "^2.4.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.9",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^3.2.0",
    "jest": "^26.3.0",
    "jest-environment-node": "^26.3.0",
    "nanoid": "^3.1.12",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@prisma/client": "^2.4.1",
    "@types/cors": "^2.8.7",
    "@types/helmet": "^0.0.47",
    "bcryptjs": "^2.4.3",
    "celebrate": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "helmet": "^4.0.0",
    "jose": "^1.28.0",
    "morgan": "^1.10.0"
  }
}
